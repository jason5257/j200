# APP_NAME
export APP=j209

# springboot port
export SPRINGBOOT_PORT=8080

#DB_RUN.sh -> springboot application.yml 참고
export MYSQL_DATABASE=j209_schema
export MYSQL_USER=user01
export MYSQL_PASSWORD=user01
export DOCKER_DB_PORT=3306

#DB_RUN.sh -> DB root password
export MYSQL_ROOT_PASSWORD=password

#DB_RUN.sh -> local machine 참고
export LOCAL_DB_PATH=/build/DB/mysql

#APP_RUN.sh -> application-docker.yml 동작
SPRING_PROFILES_ACTIVE=docker

#APP_RUN.sh -> .yml mapping:server.port
DOCKER_SPRINGBOOT_PORT=8080

#WEB_RUN.sh ->
DOCKER_WEB_PORT=80

#WEB_RUN.sh ->
DEFAULT_CONF_DIR=/build/SRC/j200/j209/scripts

########## ########## ##########
# image name
export IMAGE=${APP}_image

# network between nginx and springboot and db
export APP_NETWORK=${APP}_net

# name of springboot in docker
export DOCKER_WAS=${APP}

# name of db in docker 
export DOCKER_DB=${APP}_db

# web name
export WEB=${APP}_web




